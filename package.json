{
  "name": "media-uploader",
  "description": "An Angular5 and Node Express with SequelizeJS and MySQL application for Image & Video Uploading using multer package",
  "main": "index.js",
  "scripts": {
    "install-ngx-express-deps": "npm install && cd ./public/uploading-app && npm install",
    "ng-start": "cd ./public/uploading-app && tsc && npm start",
    "express-start": "tsc-watch --onSuccess 'nodemon ./bin/www'",
    "start": "concurrently \"npm run express-start\"  \"npm run ng-start\"",
    "knex:latest": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:latest",
    "knex:rollback": "babel-node --extensions '.ts' node_modules/.bin/knex migrate:rollback"
  },
  "author": "Kristy Mae Almuete <kristyalmuete@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KShewengerz/media-uploader"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~4.1.0",
    "dotenv": "^6.2.0",
    "event-stream": "^3.3.3",
    "express": "^4.16.4",
    "knex": "^0.14.6",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "serve-favicon": "~2.5.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.1.0",
    "@angular/compiler": "^7.1.1",
    "@angular/compiler-cli": "^7.1.1",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0-beta.45",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0-beta.45",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jasmine": "^3.3.0",
    "@types/knex": "^0.15.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.11",
    "@types/passport": "^0.4.7",
    "@types/passport-google-oauth": "^1.0.38",
    "concurrently": "^4.1.0",
    "mime": "^2.4.0",
    "nodemon": "^1.18.7",
    "tsc-watch": "^1.0.31",
    "tslint": "^5.9.1",
    "typescript": "^3.1.1",
    "typescript-register": "^1.1.0",
    "webpack-dev-server": "^3.1.14"
  }
}
